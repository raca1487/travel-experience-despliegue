<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
	xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">

	<mapped-superclass class="es.diverplan.trex.Valoracion" access="FIELD">
		<attributes>
			<basic name="comentario" optional="false">
				<column name="COMENTARIOS" />
			</basic>
			<basic name="puntuacion" optional="false">
				<column name="PUNTUACIONES" />
			</basic>
			<transient name="fechaPublicacion" />
		</attributes>
	</mapped-superclass>
</entity-mappings>